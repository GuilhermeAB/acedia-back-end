{
  "name": "acedia-back-end",
  "description": "acedia - template back end",
  "version": "1.0.0",
  "author": "Guilherme Afonso Borchardt",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "serve": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"yarn watch-ts\" \"yarn serve-debug\"",
    "serve-debug": "nodemon --inspect dist/index.js",
    "watch-ts": "tsc -w"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "date-fns": "^2.23.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-validator": "^6.12.1",
    "helmet": "^4.6.0",
    "lodash": "^4.17.21",
    "mongoose": "^6.0.4",
    "morgan": "^1.10.0",
    "request-ip": "^2.1.3",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/compression": "^1.7.1",
    "@types/concurrently": "^6.2.1",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/lodash": "^4.14.172",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.7.10",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/experimental-utils": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "@typescript-eslint/typescript-estree": "^4.30.0",
    "concurrently": "^6.2.1",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-plugin-import": "^2.24.2",
    "nodemon": "^2.0.12",
    "rimraf": "^3.0.2",
    "ts-node": "^10.2.1",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.2"
  },
  "packageManager": "yarn@3.1.0-rc.2"
}
